    "faq_section_Wy9pQG": {
      "type": "faq-section",
      "blocks": {
        "item_NrBeKt": {
          "type": "item",
          "settings": {
            "a-heading": "Was ist Goldenbody?",
            "a-content": "Goldenbody ist dein Partner für Fitness, Ernährung und ganzheitliches Wohlbefinden. Wir bieten individuelle Trainings, Ernährungscoaching, Challenges und vieles mehr, um dich bei deinen Zielen zu unterstützen."
          }
        },
        "item_3MqiNj": {
          "type": "item",
          "settings": {
            "a-heading": "Welche Services bietet Goldenbody an?",
            "a-content": "Wir bieten Personal Training, Ernährungsberatung, Online Challenges, Gruppenkurse, Retreats und exklusive Wellness-Produkte an."
          }
        },
        "item_HH49Bz": {
          "type": "item",
          "settings": {
            "a-heading": "Wer kann bei Goldenbody mitmachen?",
            "a-content": "Egal, ob Anfänger oder Fortgeschrittene, bei uns ist jeder willkommen! Unsere Programme werden an dein Fitnesslevel angepasst."
          }
        },
        "item_ixd9Eq": {
          "type": "item",
          "settings": {
            "a-heading": "Was sind die Goldenbody Challenges?",
            "a-content": "Unsere 4- und 8-Wochen-Challenges helfen dir, gezielt Gewicht zu verlieren, Muskeln aufzubauen oder fitter zu werden – alles online mit umfassender Betreuung."
          }
        },
        "item_xJ7N7p": {
          "type": "item",
          "settings": {
            "a-heading": "Wie funktioniert das Personal Training bei Goldenbody?",
            "a-content": "Unser Personal Training ist individuell auf dich abgestimmt. Du trainierst mit erfahrenen Trainern, die dich motivieren und unterstützen."
          }
        },
        "item_dAjnQF": {
          "type": "item",
          "settings": {
            "a-heading": "Welche Produkte verkauft Goldenbody?",
            "a-content": "Wir bieten eine Auswahl an hochwertigen Wellness- und Fitness-Produkten sowie Ernährungs- und Fitness-E-Books."
          }
        },
        "item_rcGhxy": {
          "type": "item",
          "settings": {
            "a-heading": "Bietet Goldenbody auch Gruppenkurse an?",
            "a-content": "Ja, neben Personal Training bieten wir Gruppenkurse im Studio und Outdoor an, die für jedes Fitnesslevel geeignet sind."
          }
        },
        "item_FtGJm4": {
          "type": "item",
          "settings": {
            "a-heading": "Welche Zahlungsmethoden akzeptiert Goldenbody?",
            "a-content": "Wir akzeptieren Kreditkarten, Twint, PayPal und Banküberweisungen für alle unsere Dienstleistungen und Produkte."
          }
        },
        "item_dtnxMA": {
          "type": "item",
          "settings": {
            "a-heading": "Gibt es eine Online-Betreuung bei Goldenbody?",
            "a-content": "Ja, über unsere Challenges und Online-Beratung kannst du von überall aus betreut werden – flexibel und ortsunabhängig."
          }
        },
        "item_4Xxgtf": {
          "type": "item",
          "settings": {
            "a-heading": "Wie melde ich mich für ein Programm oder eine Challenge an?",
            "a-content": "Einfach auf unserer Website das passende Programm auswählen, dich anmelden und sofort starten."
          }
        },
        "item_jpehWW": {
          "type": "item",
          "settings": {
            "a-heading": "Wie kann ich das Team von Goldenbody kontaktieren?",
            "a-content": "Du kannst uns jederzeit per E-Mail oder über unser Kontaktformular auf der Website erreichen. Wir antworten in der Regel innerhalb von 24 Stunden."
          }
        },
        "item_TTVGci": {
          "type": "item",
          "settings": {
            "a-heading": "Kann ich eine Probestunde buchen?",
            "a-content": "Ja, du kannst für unsere Gruppenkurse  eine Probestunde buchen. Melde dich einfach über die Website an."
          }
        },
        "item_Hj8chH": {
          "type": "item",
          "settings": {
            "a-heading": "Bietet Goldenbody auch Beratungen vor Ort an?",
            "a-content": "Ja, du kannst neben unseren Online-Angeboten auch persönliche Beratungen und Trainings vor Ort im Goldenbody-Studio buchen."
          }
        }
      },
