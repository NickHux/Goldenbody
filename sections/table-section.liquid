<section class="table-sec" id="{{ section.settings.section-id }}">
  <div class="container">
        <div class="row">
            <div class="col">
                <div class="table-contain">
                    <div class="table_component" role="region" tabindex="0">
                        <table>
                            <thead>
                                <tr>
                                    <th><span>{{ section.settings.heading-1 }}</span></th>
                                    <th><span>{{ section.settings.heading-2 }}</span></th>
                                    <th><span>{{ section.settings.heading-3 }}</span></th>
                                    <th><span>{{ section.settings.heading-4 }}</span></th>
                                    <th><span>{{ section.settings.heading-5 }}</span></th>
                                </tr>
                            </thead>
                            <tbody>
                                {% if section.blocks != blank %}
                                {% for block in section.blocks %}
                                <tr>
                                    <td>{{ block.settings.heading }}</td>
                                    <td class="p-{{ block.settings.option-1 }} points"><span></span> <span></span> <span></span> <span></span> <span></span></td>
                                    <td class="p-{{ block.settings.option-2 }} points"><span></span> <span></span> <span></span> <span></span> <span></span></td>
                                    <td class="p-{{ block.settings.option-3 }} points"><span></span> <span></span> <span></span> <span></span> <span></span></td>
                                    <td class="p-{{ block.settings.option-4 }} points"><span></span> <span></span> <span></span> <span></span> <span></span></td>
                                </tr>
                                {% endfor %}
                                {% endif %}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

  

{% schema %}
    {
    "name": "Table-sec",
    "class": "Table-sec",
    "settings": [
    {
    "type": "text",
    "id": "section-id",
    "label": "Section Id"
    },
    {
      "type": "text",
      "id": "heading-1",
      "label": "Heading 1"
    },
    {
      "type": "text",
      "id": "heading-2",
      "label": "Heading 2"
    },
    {
      "type": "text",
      "id": "heading-3",
      "label": "Heading 3"
    },
    {
      "type": "text",
      "id": "heading-4",
      "label": "Heading 4"
    },
    {
      "type": "text",
      "id": "heading-5",
      "label": "Heading 5"
    } 
    ], 
    "blocks": [
      {
        "type": "slides",
        "name": "slides",
        "settings": [
          {
            "type": "range",
            "id": "option-1",
            "label": "Option 1",
            "min": 0,
            "max": 5,
            "step": 1,
            "default": 0
          },
          {
            "type": "range",
            "id": "option-2",
            "label": "Option 2",
            "min": 0,
            "max": 5,
            "step": 1,
            "default": 0
            
          },
          {
            "type": "range",
            "id": "option-3",
            "label": "Option 3",
            "min": 0,
            "max": 5,
            "step": 1,
            "default": 0
          },
          {
            "type": "range",
            "id": "option-4",
            "label": "Option 4",
            "min": 0,
            "max": 5,
            "step": 1,
            "default": 0
          },
          {
            "type": "text",
            "id": "heading",
            "label": "Heading"
          }
        ]
      }
    ], 
    "presets": [
    {
    "category": "Table-sec",
    "name": "Table-sec"
    }
    ]
    }
{% endschema %}